{
  "name": "pokerquarantine-master",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "cd client && npm i && npm run build && cd ..",
    "start": "node ./bin/www",
    "server": "nodemon ./bin/www --ignore client/",
    "client": "cd client && npm start",
    "testClient": "concurrently \"npm run server\" \"sleep 5 && cd client && npm run test\"",
    "testServer": "mocha --recursive --exit",
    "local": "concurrently \"npm run server\" \"npm run client\""
  },
